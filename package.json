{
  "name": "malatium",
  "version": "0.0.1",
  "description": "flyd and mithril starter",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:js",
    "build:js": "set NODE_ENV=develop && webpack",
    "watch": "parallelshell \"npm run dev\" \"npm run watch:js\"",
    "watch:js": "webpack -w",
    "min": "set NODE_ENV=production && webpack -p --config webpack.production.config.js",
    "dev": "nodemon server.js",
    "start": "NODE_ENV=production forever start server.js",
    "test": "eslint . && ava"
  },
  "author": "Zach Dahl <z.schtauffen@gmail.com>",
  "license": "ISC",
  "ava": {
    "files": [
      "src/**/*-spec.js"
    ],
    "failFast": true,
    "verbose": true,
    "require": [
      "babel-core/register"
    ],
    "babel": "inherit"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "axis": "^0.6.1",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-module-resolver": "^2.2.0",
    "babel-plugin-syntax-trailing-function-commas": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.10.2",
    "babel-plugin-transform-function-bind": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.3.13",
    "babel-plugin-transform-runtime": "^6.5.2",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.3.13",
    "babelify": "^7.2.0",
    "css-loader": "^0.25.0",
    "eslint": "^3.8.1",
    "eslint-config-standard": "^6.2.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-standard": "^2.0.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "jeet": "^6.1.5",
    "nib": "^1.1.0",
    "nodemon": "^1.9.1",
    "parallelshell": "^2.0.0",
    "rupture": "^0.6.1",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.1.1",
    "uglifyjs": "^2.4.10",
    "webpack": "^1.12.14",
    "whatwg-fetch": "^1.0.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "forever": "^0.15.1",
    "koa": "^1.2.0",
    "koa-compress": "^1.0.9",
    "koa-router": "^5.4.0",
    "koa-static": "^2.0.0",
    "mithril": "github:lhorie/mithril.js#rewrite",
    "node-uuid": "^1.4.7",
    "sanctuary": "^0.11.1"
  }
}
